<h2>Leaderboard</h2>
<table class="table">
  <thead>
    <tr>
      <td>Username</td>
      <td>Avatar</td>
      <td>Score</td>
    </tr>
  </thead>
  <tbody>
    {{#each model as |user|}}
    <tr>
      <td> {{user.username}} </td>
      <td><img class="propic" src='{{user.propic}}'></td>
      <td> {{user.score}} </td>
    </tr>
    {{/each}}
  </tbody>
</table>
